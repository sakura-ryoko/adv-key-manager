{
    "schemaVersion": 1,
    "id": "${id}",
    "version": "${version}",
    "name": "${name}",
    "description": "Advanced Keybind Manager for fabric mods utilizing MaLiLib",
    "authors": [
        "Sakura-Ryoko"
    ],
    "contact": {
        "homepage": "https://modrinth.com",
        "sources": "https://github.com/sakura-ryoko/adv-key-manager"
    },
    "icon": "assets/adv_key_manager/icon.png",
    "environment": "client",
    "entrypoints": {
        "main": [
            "com.sakuraryoko.adv_key_manager.AdvKeyManager"
        ],
        "modmenu": [
            "com.sakuraryoko.adv_key_manager.compat.modmenu.ModMenuImpl"
        ]
    },
    "mixins": [
        "adv_key_manager.mixins.json"
    ],
    "accessWidener": "adv_key_manager.accesswidener",
    "depends": {
        "fabricloader": ">=0.11.4",
        "minecraft": "${minecraft_dependency}",
        "malilib": "${malilib_dependency}",
        "conditional-mixin": ">=0.2.0",
        "mixinextras": ">=0.3.0"
    }
}
